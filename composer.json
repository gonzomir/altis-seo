{
    "name": "altis/seo",
    "description": "SEO module for Altis",
    "type": "package",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Human Made",
            "email": "hello@humanmade.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "humanmade/wp-seo": "0.14.0",
        "humanmade/hm-redirects": "~0.5.1",
        "humanmade/meta-tags": "^0.1.6",
        "yoast/wordpress-seo": "^16.2.0"
    },
    "autoload": {
        "files": [
            "inc/namespace.php",
            "inc/site_verification/namespace.php"
        ]
    },
    "extra": {
        "altis": {
            "install-overrides": [
                "humanmade/wp-seo",
                "humanmade/hm-redirects",
                "humanmade/meta-tags",
                "yoast/wordpress-seo"
            ]
        }
    }
}
